{
    "$schema": "../../schema/jinks.json",
    "type": "feature",
    "id": "https://e-editiones.org/app/tei-publisher/letters",
    "label": "Labedyt letters",
    "description": "Profile for a TEI Publisher 10 application with parallel text/facsimile display for correspondence. By default shows transcription and facsimile viewer",
    "version": "1.0.0",
    "depends": [
        "base10"
    ],
    "defaults": {
        "sort": [{"field": "date"}]
    },
    "templating": {
        "use": [
            "templates/letters-blocks.html"
        ]
    },
    "features": {
        "register": {
            "enabled": true,
            "placement": "left"
        },
        "pb-tify": true
    }
}