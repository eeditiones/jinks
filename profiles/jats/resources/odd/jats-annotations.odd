    <text>
        <body>
            <schemaSpec start="TEI teiCorpus" ident="jats-annotations" source="jats.odd" ns="">
                <elementSpec ident="named-content" mode="add">
                    <model predicate="@content-type = 'person'" behaviour="inline" cssClass="annotation annotation-person authority"/>
                    <model predicate="@content-type = 'place'" behaviour="inline" cssClass="annotation annotation-place authority"/>
                    <model predicate="@content-type = 'term'" behaviour="inline" cssClass="annotation annotation-term authority"/>
                    <model predicate="@content-type = 'organization'" behaviour="inline" cssClass="annotation annotation-organization authority"/>
                    <model predicate="@content-type = 'work'" behaviour="inline" cssClass="annotation annotation-work authority"/>
                    <model behaviour="inline"/>
                </elementSpec>
                <elementSpec ident="fn" mode="change">
                    <model predicate="label" behaviour="note">
                        <param name="n" value="label/string()" />
                        <param name="content" value="node() except label" />
                    </model>
                    <model behaviour="note" />
                </elementSpec>
                <elementSpec ident="xref" mode="change">
                    <model predicate="@ref-type='bibr'" behaviour="link">
                        <param name="uri" value="'#' || @rid"/>
                    </model>
                    <model predicate="@ref-type='fn'" behaviour="note">
                        <param name="content" value="let $rid := @rid return root($parameters?root)//fn[@id=$rid]/(node() except label)"/>
                        <param name="n" value="let $rid := @rid return root($parameters?root)//fn[@id=$rid]/label"/>
                    </model>
                </elementSpec>
                <elementSpec ident="ext-link" mode="change">
                    <model behaviour="inline" cssClass="annotation annotation-ref" useSourceRendition="true"/>
                </elementSpec>
                <elementSpec ident="bold" mode="change">
                    <model behaviour="inline" cssClass="annotation annotation-hi" useSourceRendition="true"/>
                </elementSpec>
                <elementSpec ident="italic" mode="change">
                    <model behaviour="inline" cssClass="annotation annotation-hi" useSourceRendition="true"/>
                </elementSpec>
                <elementSpec ident="underline" mode="add">
                    <model behaviour="inline" cssClass="annotation annotation-hi" useSourceRendition="true"/>
                </elementSpec>
                <elementSpec ident="strike" mode="add">
                    <model behaviour="inline" cssClass="annotation annotation-hi" useSourceRendition="true"/>
                </elementSpec>
                <elementSpec ident="styled-content" mode="add">
                    <model behaviour="inline" cssClass="annotation annotation-hi" useSourceRendition="true"/>
                </elementSpec>
                <elementSpec ident="abbrev" mode="change">
                    <model behaviour="pass-through">
                        <pb:template xmlns="" xml:space="preserve"><span class="annotation annotation-abbreviation" data-type="abbreviation" data-annotation="{{&#34;expan&#34;: &#34;[[expan]]&#34;}}">[[content]]</span></pb:template>
                    </model>
                </elementSpec>
            </schemaSpec>
        </body>
    </text>
