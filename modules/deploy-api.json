{
    "components": {
        "securitySchemes": {
            "basicAuth": {
                "scheme": "basic",
                "type": "http"
            },
            "cookieAuth": {
                "in": "cookie",
                "name": "teipublisher.com.login",
                "type": "apiKey"
            }
        }
    },
    "info": {
        "title": "jinks API",
        "version": "1.0.0"
    },
    "openapi": "3.0.3",
    "paths": {
        "/api/generator/{profile}/deploy": {
            "post": {
                "description": "Deploy a profile. Call for new profiles, or to re-deploy a profile from scratch. Requires DBA privileges.",
                "operationId": "dep:deploy",
                "parameters": [
                    {
                        "name": "profile",
                        "in": "path",
                        "schema": {
                            "type": "string",
                            "example": "base"
                        },
                        "required": true,
                        "description": "The name of the profile to use"
                    }
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        },
                        "description": "OK"
                    }
                }
            }
        },
        "/api/generator/action/{action}": {
            "get": {
                "description": "Run an action",
                "operationId": "dep:run-action",
                "parameters": [
                    {
                        "name": "action",
                        "in": "path",
                        "schema": {
                            "type": "string"
                        },
                        "required": true
                    },
                    {
                        "name": "id",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        },
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        },
                        "description": "OK"
                    }
                }
            }
        }
    },
    "servers": [
        {
            "description": "Endpoint for testing on localhost",
            "url": "/exist/apps/jinks"
        }
    ],
    "security": [
        {
            "cookieAuth": []
        },
        {
            "basicAuth": []
        }
    ]
}